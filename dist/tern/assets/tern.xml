<launch4jConfig>
  <headerType>console</headerType>
  <jar>../dist/tern.jar</jar>
  <dontWrapJar>false</dontWrapJar>
  <outfile>../dist/tern.exe</outfile>
  <errTitle>Tern</errTitle>
  <icon>tern.ico</icon>
  <restartOnCrash>false</restartOnCrash>
  <jre>
     <minVersion>1.8.0</minVersion>
     <opt>-XX:+IgnoreUnrecognizedVMOptions --add-opens=java.base/jdk.internal.loader=ALL-UNNAMED</opt>          
  </jre>
</launch4jConfig>
