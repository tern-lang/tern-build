<launch4jConfig>
  <headerType>gui</headerType>
  <jar>../dist/ternd.jar</jar>
  <dontWrapJar>false</dontWrapJar>
  <outfile>../dist/ternd.exe</outfile>
  <errTitle>Tern Studio</errTitle>
  <icon>ternd.ico</icon>
  <restartOnCrash>false</restartOnCrash>
  <jre>
     <minVersion>1.8.0</minVersion>
     <opt>-XX:+IgnoreUnrecognizedVMOptions --add-opens=java.base/jdk.internal.loader=ALL-UNNAMED</opt>          
  </jre>
</launch4jConfig>
